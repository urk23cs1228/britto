git clone 
cd "folder name"

git config --user.name  ""
                 .email  ""

git config --global init.defaultBranch main


git remote add orgin <url-link>


#add files 


git add. 

git commit -m "message "


git push origin main 





                    ##### MERGING BRANCHING #########
#creata repo  
git clone 
cd "folder name"

git config --user.name  ""
                 .email  ""

git config --global init.defaultBranch main

  
git remote add origin <url-link>

git checkout -b <branch-name>

#add files /cretae files 

git add.

  git checkout main 

  git merge <branch-name>

git commit -m "message"

git push origin main




                 ##ISSUE##

#CREATE ISSUSE ON GITHUB 



git clone 

cd "folder name"

git config --user.name  ""
                 .email  ""

git config --global init.defaultBranch main
git add.

git commit -m "Fix#1: sdfsdvsvs"
git push origin main






            ### FORK A REPO 
git clone 

cd "folder name"

git config --user.name  ""
                 .email  ""

git config --global init.defaultBranch main

    git remote add upstream <url-link>

    git fetch upstream 

#add files or changes 

    git checkout main 

    

    git merge upstream/main

    git commit -m "updateed "

    git push origin  main 
